{
  "App": {
    "SelfUrl": "https://cima-staging.up.railway.app",
    "CorsOrigins": "https://cima-staging.up.railway.app",
    "RedirectAllowedUrls": "https://cima-staging.up.railway.app"
  },
  "AuthServer": {
    "Authority": "https://cima-staging.up.railway.app",
    "RequireHttpsMetadata": true,
    "IsOnK8s": "false"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning"
    }
  },
  "ImageStorage": {
    "Provider": "AzureBlob",
    "Azure": {
      "ConnectionString": "REPLACE_WITH_CONNECTION_STRING",
      "ContainerName": "listings-staging",
      "PublicBaseUrl": "CAMBIO"
    }
  },
  "Redis": {
    "Enabled": false
  },
  "OpenIddict": {
    "Applications": {
      "cima_BlazorWebApp": {
        "RootUrl": "https://cima-staging.up.railway.app",
        "ClientUri": "https://cima-staging.up.railway.app"
      },
      "cima_App": {
        "RootUrl": "https://cima-staging.up.railway.app"
      },
      "cima_Swagger": {
        "RootUrl": "https://cima-staging.up.railway.app"
      }
    }
  },
  "Email": {
    "Provider": "Smtp",
    "Smtp": {
      "Host": "CAMBIO",
      "Port": 587,
      "UserName": "",
      "Password": "",
      "EnableSsl": true,
      "UseDefaultCredentials": false,
      "DefaultFromAddress": "CAMBIO",
      "DefaultDisplayName": "Cima Staging"
    }
  }
}